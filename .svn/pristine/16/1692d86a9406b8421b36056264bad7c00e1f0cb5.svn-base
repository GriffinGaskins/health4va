public class Record
{
  private int sch_year;
  private int div_num;
  private int sch_num = 0;
  private String race;
  private String gender = "ALL";
  private String disabil = "ALL";
  private String lep = "ALL";
  private String disadva = "ALL";
  private int program_num;
  private int comp_cnt;

	public Record(int sch_year, int div_num, String race, int program_num, int comp_cnt)
	{
		this.sch_year = sch_year;
		this.div_num = div_num;
		this.race = race;
		this.program_num = program_num;
		this.comp_cnt = comp_cnt;
	}

	public int getYear()
	{
		return sch_year;
	}

	public int getDiv()
  {
    return div_num;
  }

	public String getRace()
  {
    return race;
  }

	public int getPrgNum()
  {
    return program_num;
  }

	public int getCompCnt()
  {
    return comp_cnt;
  }

}
